iner\" class=\"mb-4 d-flex flex-wrap\"><\/div>\r\n      <div id=\"categories-container\" class=\"mb-4 d-flex flex-wrap\"><\/div>\r\n      <div id=\"experiments-container\" class=\"row\"><\/div>\r\n    <\/div>\r\n\r\n    <!-- Popup for experiment details -->\r\n    <div class=\"popup-overlay\"><\/div>\r\n    <div class=\"popup\">\r\n      <h5 id=\"popup-title\"><\/h5>\r\n      <p id=\"popup-description\"><\/p>\r\n      <button class=\"btn btn-primary\" id=\"close-popup\">\u0627\u063a\u0644\u0627\u0642<\/button>\r\n    <\/div>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n<script ver=\"1\">\r\njQuery(document).ready(function ($) {\r\n  const API_URLS = {\r\n    branches: \"https:\/\/praxilabs-lms.com\/api\/external\/branches\",\r\n    experiments: (page, scienceId) =>\r\n      `https:\/\/praxilabs-lms.com\/api\/external\/experiments?page=${page}&scienceId=${scienceId}`,\r\n  };\r\n\r\n  const images = {\r\n    1: [\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/1-3.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/2-3.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/24.webp\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/4-3.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/5-3.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/7-2.png\",\r\n    ],\r\n    2: [\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/Rectangle-8687.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/Rectangle-8687-1.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/Rectangle-8687-2.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/Rectangle-8687-1.webp\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/1.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/2.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/2.webp\",\r\n    ],\r\n    3: [\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/5435345.webp\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/Rectangle-8687-1-3.webp\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/345345345.webp\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/gfdg.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/534535.png\",\r\n      \"https:\/\/hayyan.shadebox.studio\/wp-content\/uploads\/2024\/12\/34555.png\",\r\n    ],\r\n  };\r\n\r\n  const BEARER_TOKEN = \"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJhcHBsaWNhdGlvbl9rZXkiOiI0MDM4ZTZhMTY2NjA2OTVmNTQyYjM0MmZhMDU2NmJjOCIsImFjY291bnRfaWQiOiIyYmVhNjQ2Zi1hNDAxLTQ3MzktYTM2Yi0yZTA4YzZkNTYxNDIiLCJpYXQiOjE3Mzg1MTgxMjksImV4cCI6MTczODUxOTkyOX0.fKgqMt3i5AJKyIv6sAjPRPcQXBtRHKsRnkJ3m9JZ6VI\";\r\n  if (!BEARER_TOKEN) {\r\n    console.error(\"Bearer token not found.\");\r\n    return;\r\n  }\r\n\r\n  fetchBranches();\r\n\r\n  function fetchBranches() {\r\n    $.ajax({\r\n      url: API_URLS.branches,\r\n      method: \"GET\",\r\n      headers: getHeaders(),\r\n      success: handleBranchesSuccess,\r\n      error: handleError(\"branches\"),\r\n    });\r\n  }\r\n\r\n  function handleBranchesSuccess(response) {\r\n    if (response.sciences) {\r\n      populateTabs(response.sciences);\r\n    } else {\r\n      console.error(\"Unexpected API response structure:\", response);\r\n    }\r\n  }\r\n\r\n  function populateTabs(sciences) {\r\n    const $tabsContainer = $(\"#tabs-container\");\r\n    $tabsContainer.empty();\r\n\r\n    sciences.forEach((science, index) => {\r\n      const $tab = $(\"<button>\")\r\n        .addClass(\"btn btn-outline-primary mr-2 mb-2\")\r\n        .attr(\"data-id\", science.id)\r\n        .text(science.arabicTitle)\r\n        .on(\"click\", function () {\r\n          \/\/ Prevent clicking the already active tab\r\n          if ($(this).hasClass(\"active-tab\")) {\r\n            return; \/\/ Prevent the click if already selected\r\n          }\r\n\r\n          \/\/ Add active class to the selected tab\r\n          $tabsContainer.find(\"button\").removeClass(\"active-tab\");\r\n          $(this).addClass(\"active-tab\");\r\n\r\n          displayLoadingSpinner();\r\n          fetchExperiments(science.id);\r\n        });\r\n\r\n      $tabsContainer.append($tab);\r\n    });\r\n\r\n    \/\/ Select first tab by default\r\n    if (sciences.length > 0) {\r\n      $tabsContainer.find(\"button\").first().trigger(\"click\");\r\n    }\r\n  }\r\n\r\n  function displayLoadingSpinner() {\r\n    const $experimentsContainer = $(\"#experiments-container\");\r\n    const $categoriesContainer = $(\"#categories-container\");\r\n    $experimentsContainer.empty();\r\n    $categoriesContainer.empty();\r\n    $experimentsContainer.append(`\r\n      <div class=\"col-12 loading-spinner\">\r\n        <div class=\"spinner-border\" role=\"status\">\r\n          <span class=\"sr-only\">Loading...<\/span>\r\n        <\/div>\r\n      <\/div>\r\n    `);\r\n  }\r\n\r\n  function fetchExperiments(scienceId) {\r\n    let experiments = [];\r\n    let currentPage = 1;\r\n\r\n    function fetchPage(page) {\r\n      $.ajax({\r\n        url: API_URLS.experiments(page, scienceId),\r\n        method: \"GET\",\r\n        headers: getHeaders(),\r\n        success: (response) => handleExperimentsSuccess(response, page),\r\n        error: handleError(\"experiments\"),\r\n      });\r\n    }\r\n\r\n    function handleExperimentsSuccess(response, page) {\r\n      if (response.experiments) {\r\n        experiments = experiments.concat(response.experiments);\r\n\r\n        const totalCount = Number(response.count);\r\n        const totalFetched = experiments.length;\r\n\r\n        if (totalFetched < totalCount) {\r\n          fetchPage(page + 1);\r\n        } else {\r\n          displayExperiments(experiments);\r\n          populateCategories(experiments);\r\n        }\r\n      } else {\r\n        console.error(\"Unexpected response structure:\", response);\r\n      }\r\n    }\r\n\r\n    fetchPage(currentPage);\r\n  }\r\n\r\n  function populateCategories(experiments) {\r\n    const categories = [];\r\n    experiments.forEach((experiment) => {\r\n      const category = experiment.category;\r\n      if (!categories.some((cat) => cat.id === category.id)) {\r\n        categories.push(category);\r\n      }\r\n    });\r\n\r\n    const $categoriesContainer = $(\"#categories-container\");\r\n    $categoriesContainer.empty();\r\n\r\n    categories.forEach((category) => {\r\n      const $categoryTab = $(\"<button>\")\r\n        .addClass(\"btn btn-outline-secondary mr-2 mb-2\")\r\n        .attr(\"data-category-id\", category.id)\r\n        .text(category.arabicTitle)\r\n        .on(\"click\", function () {\r\n          \/\/ Prevent clicking the already active category\r\n          if ($(this).hasClass(\"active-category\")) {\r\n            return; \/\/ Prevent the click if already selected\r\n          }\r\n\r\n          \/\/ Add active class to the selected category\r\n          $categoriesContainer.find(\"button\").removeClass(\"active-category\");\r\n          $(this).addClass(\"active-category\");\r\n          filterExperimentsByCategory(category.id, experiments);\r\n        });\r\n\r\n      $categoriesContainer.append($categoryTab);\r\n    });\r\n\r\n    \/\/ Select first category by default\r\n    if (categories.length > 0) {\r\n      $categoriesContainer.find(\"button\").first().trigger(\"click\");\r\n    }\r\n  }\r\n\r\n  function filterExperimentsByCategory(categoryId, experiments) {\r\n    const filteredExperiments = experiments.filter(\r\n      (exp) => exp.category.id === categoryId\r\n    );\r\n    displayExperiments(filteredExperiments);\r\n  }\r\n\r\n  function displayExperiments(experiments) {\r\n    const $experimentsContainer = $(\"#experiments-container\");\r\n    $experimentsContainer.empty();\r\n\r\n    if (experiments.length === 0) {\r\n      $experimentsContainer.append(\r\n        `<div class=\"col-12\"><p class=\"text-center\">No experiments available.<\/p><\/div>`\r\n      );\r\n      return;\r\n    }\r\n    let randomImagesNumber = Math.floor(Math.random() * (100 - 1 + 1)) + 1;\r\n    experiments.forEach((experiment, i) => {\r\n\t\tlet imageCatNumber = [\"1\",\"2\",\"3\"].includes(experiment.scienceId) ? experiment.scienceId : 1;\r\n      const $experimentCard = `\r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"card h-100\">\r\n            <img decoding=\"async\"\r\n              src=\"${images[imageCatNumber][(i+randomImagesNumber)%images[imageCatNumber].length]}\"\r\n              class=\"card-img-top\"\r\n              alt=\"${experiment.arabicTitle}\"\r\n            \/>\r\n            \r\n            <div class=\"card-body text-center\">\r\n              <h5 class=\"card-title\">${experiment.arabicTitle}<\/h5>\r\n              <button class=\"btn  view-details ${\r\n                experiment.arabicDescription ? \"\" : \"d-none\"\r\n              }\" data-id=\"${experiment.id}\">\u0627\u0644\u0645\u0632\u064a\u062f<\/button>\r\n            <\/div>\r\n          <\/div>\r\n        <\/div>\r\n      `;\r\n\r\n      $experimentsContainer.append($experimentCard);\r\n    });\r\n\r\n    \/\/ Open popup when 'View Details' button is clicked\r\n    $(\".view-details\").on(\"click\", function () {\r\n      const experimentId = $(this).data(\"id\");\r\n      const experiment = experiments.find(\r\n        (exp) => exp.id === String(experimentId)\r\n      );\r\n      if (experiment) {\r\n        $(\"#popup-title\").text(experiment.arabicTitle);\r\n        $(\"#popup-description\").html(experiment.arabicDescription);\r\n        $(\".popup-overlay, .popup\").fadeIn();\r\n      }\r\n    });\r\n  }\r\n\r\n  function handleError(type) {\r\n    return function () {\r\n      displayErrorMessage(`.\u0641\u0634\u0644 \u062a\u062d\u0645\u064a\u0644 \u0628\u064a\u0627\u0646\u0627\u062a`);\r\n      $(\"#refresh-button\").removeClass(\"d-none\"); \/\/ Show the refresh button\r\n    };\r\n  }\r\n\r\n  function displayErrorMessage(message) {\r\n    const $errorMessage = $(\"#error-message\");\r\n    $errorMessage.text(message).removeClass(\"d-none\");\r\n  }\r\n\t\r\n\t$(\"#close-popup\").on(\"click\", function () {\r\n          $(\".popup-overlay, .popup\").fadeOut();\r\n        });\r\n\r\n  \/\/ Refresh page when the refresh button is clicked\r\n  $(\"#refresh-button\").on(\"click\", function () {\r\n    location.reload(); \/\/ Reload the page\r\n  });\r\n\r\n  function getHeaders() {\r\n    return {\r\n      \"Authorization\": `Bearer ${BEARER_TOKEN}`,\r\n    };\r\n  }\r\n});\r\n\r\n<\/script>\r\n\r\n\r\n<style>\r\n      .loading-spinner {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        min-height: 200px;\r\n      }\r\n\r\n      .spinner-border {\r\n        width: 3rem;\r\n        height: 3rem;\r\n        color: #9951a1;\r\n      }\r\n      #tabs-container,\r\n      #categories-container {\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        align-items: center;\r\n        direction: rtl;\r\n      }\r\n      #experiments-container {\r\n        direction: rtl;\r\n      }\r\n      #tabs-container button,\r\n      #categories-container button {\r\n        padding: 10px 15px;\r\n        cursor: pointer;\r\n        font-size: 16px;\r\n        border-radius: 30px;\r\n        background-color: #f5f5f5;\r\n        border: none;\r\n        padding: 10px 15px;\r\n        cursor: pointer;\r\n        font-size: 16px;\r\n        border-radius: 30px;\r\n        transition: all 0.3s;\r\n        color: #575757;\r\n        text-align: center;\r\n        font-weight: bold;\r\n\t\tmargin: 0px 5px;\r\n      }\r\n      #categories-container button {\r\n        font-size: 14px;\r\n        font-weight: 400;\r\n      }\r\n\r\n      .card {\r\n        transition: transform 0.2s, box-shadow 0.2s;\r\n        border: none;\r\n      }\r\n      .card h5 {\r\n        text-align: center;\r\n        color: #252f40;\r\n        font-family: \"Roboto\", Sans-serif;\r\n        font-weight: 700;\r\n        font-size: 16px;\r\n      }\r\n\r\n      .card img {\r\n        border-radius: 14px;\r\n      }\r\n      .card-body {\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n      }\r\n      .view-details,\r\n      #close-popup {\r\n        width: 100px;\r\n        background-color: #8b58dd;\r\n        color: white;\r\n        border: none;\r\n      }\r\n      .view-details:hover,\r\n      #close-popup:hover {\r\n        background-color: #9951a1;\r\n        color: white;\r\n      }\r\n\r\n      .card:hover {\r\n        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);\r\n        border-radius: 30px;\r\n      }\r\n\r\n      .popup {\r\n        display: none;\r\n        position: fixed;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(-50%, -50%);\r\n        background-color: white;\r\n        padding: 20px;\r\n        border: 2px solid #8b58dd;\r\n        z-index: 1050;\r\n        box-shadow: 0 0 15px rgba(0, 0, 0, 0.1);\r\n        max-height: 80%;\r\n        overflow-y: scroll;\r\n        width: 85%;\r\n        max-width: 800px;\r\n      }\r\n      #popup-title {\r\n        text-align: center;\r\n        font-size: 16px;\r\n      }\r\n      #popup-description {\r\n        text-align: right;\r\n        direction: rtl;\r\n        font-size: 14px;\r\n      }\r\n\r\n      .popup-overlay {\r\n        display: none;\r\n        position: fixed;\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 100%;\r\n        background-color: rgba(0, 0, 0, 0.5);\r\n 